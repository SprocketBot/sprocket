{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true
	},
	"typeAcquisition": {
		"include": ["src/app.d.ts"]
	},
	"paths": {
		"$lib": ["src/lib"],
		"$lib/*": ["src/lib/*"]
	}
}
